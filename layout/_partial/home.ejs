<section id="intro-section">
		<div class="pure-g m-content">
		<div class="pure-u-1 " >
			<div class="text-content" >
				<p class="home-title">
					<%=theme.intro.title%>
				</p>
				<p class="sub">
					<%=theme.intro.sub%>
				</p>
				<p class="sub-content">
					<%
					var current=0;
					%>
					<% Object.keys(theme.home).forEach(key => { %>
						<%if(current!==0){%> / <%}%> <a href="<%=url_for(theme.home[key])%>"  class="hover-link"><%=key%></a>
						<%
						current=current+1;
						%>
					  <% }); %>
				</p>
				<p class="intro-contact">
					<% for(var y in theme.contact){ %>
						<a href="<%=theme.contact[y][0]%>" target="_blank">
							<i class="<%=theme.contact[y][1]%>"></i>
						</a>
					<% } %>
				</p>
			</div>

		</div>
    </div>
</section>
	
